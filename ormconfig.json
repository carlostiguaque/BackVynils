{
    "type": "postgres",
    "url": process.env.DATABASE_URL,
    "synchronize": false,
    "logging": false,
    "entities": [
        "src/**/*entity.ts"
    ],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ],
    "cli": {
        "entitiesDir": "src/",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    }
}